	ToggleButton {
      inset  = "1 0 0 0"
      inset  = "1 3 0 0" [$OSX]
      font-family=basefont
      font-size=13
      font-weight=400
      textcolor  = "Text"
      bgcolor=none
      render_bg
      {
      
        // background fill
	   0  = "gradient( x0 + 1, y0 + 1, x1 - 1, y1 - 1, blue, green )"
			
