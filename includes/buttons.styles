					47="fill(x0+1, y1, x0+2, y1+1, grey25)" // bottom left
					48="fill(x0, y1-1, x0+1, y1, grey25)" // bottom left
					49="fill(x0+2, y1, x1-2, y1+1, grey50)" // single 1px line.			
	
				}
			}
			
			Button:selected { // Altered to fix some buttons inside Settings not showing
				font-family=basefont
				font-size=13
				textcolor="white"
				
			//	font-style="outerglow"
			//	font-outerglow-color="darkGreyEnd"
			//	font-outerglow-offset=1
			//	font-outerglow-filtersize=1
			
				textshadow="red"
				
				padding-left=3
				padding-top=2
				padding-bottom=1
					
				bgcolor=none
				
				render_bg {
			        // background fill
			        0 = "gradient( x0 + 1, y0 + 1, x1 - 1, y1 - 1, grey, lightGreyEnd )" // Background Gradient
			                
			        // Dark Borders around
